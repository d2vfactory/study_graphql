type User {
    id: Long!
    name: String!
    age: Int!
}

extend type Query {
    users: [User]!
    user(id: Long): User!
    countUsers: Long!
}

extend type Mutation {
    newUser(name: String!, age: Int!): User!
    deleteUser(id: Long!): Boolean
    updateUserName(id: Long!, name: String!): User!
    updateUserAge(id: Long!, age: Int!): User!
}
